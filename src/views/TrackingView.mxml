<?xml version="1.0" encoding="utf-8"?>
<s:View xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" title="TrackingView">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			import myComponents.IListElement;
			private function getItemRenderer(item:Object):ClassFactory {
				return (item as IListElement).trackingItemRendererFunction();
			}
		]]>
	</fx:Script>
	<s:List id = "trackinglist" 
			dataProvider="{ModelLocator.getInstance().trackingList}"
			itemRendererFunction="getItemRenderer">
		
	</s:List>
</s:View>
